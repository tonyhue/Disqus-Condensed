{
  "name": "Disqus Condensed",
  "version": "1.1.4",
  "manifest_version": 2,
  "description": "Condensed theme for enhanced readability when browsing disqus.com/home/",
  "homepage_url": "https://disqus.com/home/",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "https://disqus.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://disqus.com/*"
      ],
      "js": [
        "js/jquery.min.js",
        "src/bg/condensing.js",
        "src/bg/onStartup.js"
      ]
    }
  ]
}